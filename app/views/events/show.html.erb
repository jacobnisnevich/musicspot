<% provide(:title, @event.name) %>

<h1> <%= @event.name %> </h1>
<div>
  <div class="event-subtitle"> Location </div>
  <div class="event-data"> <%= @event.location %> </div>
  <div class="event-subtitle"> Start </div>
  <div class="event-data"> <%= @event.start_datetime %> </div>
  <div class="event-subtitle"> End </div>
  <div class="event-data"> <%= @event.end_datetime %> </div>
  <div class="event-subtitle"> Description </div>
  <div class="event-data"> <%= @event.description %> </div>
</div>